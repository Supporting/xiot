DEF _Transform Transform {
	children [
		DEF _Shape Shape {geometry DEF _Sphere Sphere {}}
		
		Transform {
			
			translation 2 0 0
			children[DEF Joe Shape {geometry Sphere {radius 0.2}}]
		}
		
		Transform {
			
			translation -2 0 0
			children[USE Joe]
		}
	]
}